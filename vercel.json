{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/reddit/:word",
      "destination": "/?utm_source=reddit&utm_medium=social&utm_campaign=:word",
      "permanent": false
    },
    {
      "source": "/reddit",
      "destination": "/?utm_source=reddit&utm_medium=social&utm_campaign=main",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "https://app-onesimple.vercel.app/:path*"
    },
    {
      "source": "/app/(.*)",
      "destination": "https://app-onesimple.vercel.app/"
    },
    { "source": "/:path*", "destination": "/:path*" }
  ]
}
