{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/app/:match*",
      "destination": "https://app-onesimple.vercel.app/:match*"
    },
    { "source": "/:match*", "destination": "/:match*" }
  ]
}
