{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/app/", "destination": "https://app-onesimple.vercel.app/" },
    {
      "source": "/app/:match*",
      "destination": "https://app-onesimple.vercel.app/:match*"
    },
    { "source": "/", "destination": "/" },
    {
      "source": "/:match*",
      "destination": "/:match*"
    }
  ]
}
