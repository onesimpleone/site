{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "https://app-onesimple.vercel.app/:path*"
    },
    {
      "source": "/app/(.*)",
      "destination": "https://app-onesimple.vercel.app/"
    },
    { "source": "/:path*", "destination": "/:path*" }
  ]
}
